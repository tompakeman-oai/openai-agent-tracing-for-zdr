{
  "name": "chatkit-implementation",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "dev-server": "npm --prefix api run dev",
    "dev-client": "next dev --turbopack",
    "dev": "concurrently \"npm run dev-server\" \"npm run dev-client\"",
    "install-client": "npm install",
    "install-server": "npm --prefix api run install",
    "install-app": "npm install -D concurrently && concurrently \"npm run install-client\" \"npm run install-server\" "
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.2.0",
    "@mui/material": "^7.2.0",
    "chart.js": "^4.5.0",
    "express": "^5.1.0",
    "http-proxy-middleware": "^3.0.5",
    "next": "15.4.5",
    "nodemon": "^3.1.10",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/express": "^5.0.3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.2.0",
    "eslint": "^9",
    "eslint-config-next": "15.4.5",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
